     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Crear un grafo vacío\n",
    "G = nx.Graph()\n",
    "\n",
    "# Agregar nodos\n",
    "G.add_node(1)\n",
    "G.add_nodes_from([2, 3])\n",
    "\n",
    "# Agregar bordes\n",
    "G.add_edge(1, 2)\n",
    "G.add_edges_from([(1, 3), (2, 3)])\n",
    "\n",
    "# Dibujar el grafo\n",
    "nx.draw(G, with_labels=True)\n"
   ]
